{
  "name": "pacoorozco/gamify-l5",
  "description": "Gamification platform that implements a serious game mechanic",
  "keywords": [
    "gamification",
    "GoW",
    "gamify",
    "game platform",
    "serious games",
    "laravel"
  ],
  "homepage": "https://github.com/pacoorozco/gamify-l5",
  "license": "GPL-2.0",
  "type": "project",
  "authors": [
    {
      "name": "Paco Orozco",
      "email": "paco@pacorozco.info",
      "homepage": "https://pacoorozco.info",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/pacoorozco/gamify-l5/issues"
  },
  "require": {
    "php": "^7.1.3",
    "laravel/framework": "5.2.*",
    "codesleeve/laravel-stapler": "1.0.*",
    "cviebrock/eloquent-sluggable": "~4.0",
    "laravelcollective/html": "^5.2.0",
    "yajra/laravel-datatables-oracle": "~6.0",
    "rtconner/laravel-tagging": "~2.2"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Gamify\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
